{
  "name": "ai-pop-culture-news",
  "version": "1.0.0",
  "description": "AI-powered pop culture news application",
  "type": "module",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r dev",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "start:worker": "pnpm --filter @ai-pop-culture-news/worker start",
    "dev:worker": "pnpm --filter @ai-pop-culture-news/worker dev"
  },
  "keywords": [
    "ai",
    "pop-culture",
    "news",
    "automation",
    "wordpress"
  ],
  "author": "AI Culture News Team",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  }
}
